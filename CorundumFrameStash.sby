[options]
mode bmc
depth 30

[engines]
smtbmc

[script]
# read_verilog
#   -formal
#     Enable support for SystemVerilog assertions and some Yosys extensions
#     replace the implicit -D SYNTHESIS with -D FORMAL.
#

read_verilog -sv -formal CorundumFrameStash.sv
prep -top CorundumFrameStash

[files]
